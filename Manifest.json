{
//basic information
"name": "Dio tab recovery" ,
"version": "1.0",
"manifest_version":2,
"default_title": "It's me! Dio!",
"icons": {
"icon.png"
},
//declare pages
"browser_action":{
"default_icon": "icon.png",
"popup": "popup.html"
},
//ask for permissions
"permissions": [
"http://*/", 
"https://*/",
"tabs",
"history"
]

}
