
{
	"manifest_version": 2,
	"name": "Destini's chrome extension",
	"description": "Undo Closed Tabs",
	"version": "0.0.1",
	"content_scripts": [
	{
	"matches": [
		"<all_urls>"
	],
		"js"["content.js"]
	}
	],
	"browser_action": {
		"default_icon": "icon_16.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "icon_16.png"
},
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"scripts": [
			"background.html"
		],
		"persistent": false
	}

	
		}
		
