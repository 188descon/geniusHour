
{
	"manifest_version": 2,
	"name": "Destini's chrome extension",
	"description": "Undo Closed Tabs",
	"version": "0.0.1",
	"browser_action": {
		"default_icon": "icon_16.png"
	},
	"icons": {
		"16": "icon_16.png",
		"32": "icon_32.png"
},
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"scripts": [
			"background.html"
		],
		"persistent": false
	}

	
		}
		
