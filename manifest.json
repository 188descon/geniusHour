{
//information about the extension
manifest_version: 2,
"name": "Dio sound effect"
"description": "The extension plays the time stop effect from JoJO"
"version": "1.0",
//allowing notifications
"permissions": [
   "notifications"
   ],
    //install a background page
    "background": {
       "scripts":[
       "background.js"
       ]
       "persistent": false
       },
      //install our content page
      "content_scripts":[{
      "matches":
      [
      <all urls>
      ]
      "js": [jquery.js, content.js]
      }
